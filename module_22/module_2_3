# This is a sample Python script.
my_list = [42, 69, 322, 13, 0, 99, -5, 9, 8, 7, -6, 5]
print(my_list)
index=0
while index<len(my_list):
    s=my_list [index]
    if s>0:
        print (s)
        index=index+1
        continue
    else:
        if s==0:
            index=index+1
            continue
        else:
            break
